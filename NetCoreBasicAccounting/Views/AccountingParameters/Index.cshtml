@model IEnumerable<NetCoreBasicAccounting.Data.Entities.AccountingParameter>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

@if (!Model.Any())
{
    <p>
        <a asp-action="Create">Create New</a>
    </p>
}

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ProcessYear)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Month)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MajorizationProcessed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rnc)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MonthFiscalClose)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProcessYear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MajorizationProcessed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rnc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonthFiscalClose)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
